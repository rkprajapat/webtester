<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
    </configSections>
    <connectionStrings>
        <add name="WebTester.Properties.Settings.WebTesterDBConnectionString"
            connectionString="Data Source=localhost;Initial Catalog=WebTesterDB;Integrated Security=True"
            providerName="System.Data.SqlClient" />
    </connectionStrings>
  <appSettings>
    <add key="smtp_server" value="smtp.webtester.com" />
    <add key="smtp_port" value="25"/>
    <add key="from_email" value="webtester@webtester.com" />
  </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
</configuration>